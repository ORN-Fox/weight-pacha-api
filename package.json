    "nodemon": "^3.0.2",
    "npm": "^11.6.4",
    "prettier": "^3.7.4",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "^2.1.1",
    "ts-node": "^10.9.2",
